{
  "name": "sass-gradient-patterns",
  "version": "0.1.1",
  "description": "CSS3 gradient pattern Sass mixins",
  "keywords": [
    "sass",
    "gradient"
  ],
  "ignore": [
    "sassdoc",
    "scripts",
    "**/.*",
    "test"
  ],
  "files": [
    "_gradient_patterns.scss",
    "LICENSE",
    "README.md"
  ],
  "main": "_gradient_patterns.scss",
  "scripts": {
    "test": "./scripts/test.sh",
    "sassdoc": "./scripts/sassdoc.sh"
  },
  "eyeglass": {
    "needs": ">= 0.8.2",
    "sassDir": "./_mq.scss",
    "exports": false
  },
  "repository": "https://github.com/helixbass/sass-gradient-patterns.git",
  "bugs": {
    "url": "https://github.com/helixbass/sass-gradient-patterns/issues"
  },
  "homepage": "http://helixbass.net/projects/sass_gradient_patterns",
  "author": "Julian Rosse <julian@helixbass.net",
  "license": "MIT",
  "devDependencies": {
    "sassdoc": "^2.3.0"
  }
}
